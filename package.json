{
  "name": "project-website",
  "private": true,
  "version": "v1.0.6",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:stage": "tsc -b && vite build --mode staging",
    "build:dev": "vite build --mode develop",
    "lint": "pnpm fmt:check && tsc --noEmit && eslint . --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "lint:ci": "tsc --noEmit && eslint . --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "fmt": "prettier --write .",
    "fmt:check": "prettier --check .",
    "test": "echo 'test...'"
  },
  "dependencies": {
    "@splidejs/react-splide": "^0.7.12",
    "lottie-react": "^2.4.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-player": "^2.16.0",
    "react-router-dom": "^7.7.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.0",
    "eslint-plugin-react-dom": "^1.53.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-react-x": "^1.52.3",
    "globals": "^16.4.0",
    "postcss": "^8.5.3",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.46.1",
    "vite": "^7.0.6"
  }
}
